#!/bin/bash

# Run migrations and start uvicorn
/init_back &
 
# Start nginx
/init_front &

# Start rq worker
/init_worker &

# Start rq scheduler
/init_scheduler &

# Start watchdog
/init_watchdog &

# Start dashboard
/init_dashboard &

# Wait for any process to exit
wait -n

# Exit with status of process that exited first
exit $?
